	Name = "Text ESP",
	Callback = function(v)
getgenv().TextESP = v
end})
Tabs.Main:AddToggle({
	Name = "Distance ESP",
	Callback = function(v)
getgenv().DistanceESP = v
end})
Tabs.Main:AddToggle({
	Name = "Rainbow ESP",
	Callback = function(v)
getgenv().RainbowESP = v
end})
